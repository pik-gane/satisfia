{
    "name": "Satisfia Dev Container",
    "image": "nordbyerik/satisfia_mujoco_gpu",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools",
                "ms-python.python"
            ]
        }
    },
    "remoteUser": "vscode",
    "features": {
        "ghcr.io/devcontainers/features/common-utils:2": {
            "installZsh": "true",
            "username": "vscode",
            "upgradePackages": "true"
        },
        "ghcr.io/devcontainers/features/node:1": {}
    }
}
